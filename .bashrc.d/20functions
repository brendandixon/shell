#---------------------------------------------------------------------------------------
# Handy functions
#---------------------------------------------------------------------------------------
function pp()
{
    popd >/dev/null 2>/dev/null
    if [ $? -eq 1 ] ; then
        cd ~
    fi
}

function pd()
{
    pushd "$@" >/dev/null
}

#---------------------------------------------------------------------------------------
# Docker
#---------------------------------------------------------------------------------------
function set_docker_machine {
  eval "$(docker-machine env $1)"
}
